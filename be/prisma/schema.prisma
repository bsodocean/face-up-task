
datasource db {
  provider = "sqlite"  // Use SQLite as the database provider
  url      = "file:./dev.db"  // Path to the SQLite database file
}


generator client {
  provider = "prisma-client-js"  
}

// Define your Alert model
model Alert {
  id        Int      @id @default(autoincrement())
  name      String
  age       Int
  fileUrl   String?
  createdAt DateTime @default(now())
}
